{
  "name": "minipopsnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "precise-commits": "precise-commits"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precise-commits"
    }
  },
  "xcodeSchemes": {
    "Debug": [
      "Debug"
    ],
    "projectDirectory": "ios"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "presets": ["react-native"]
  },
  "eslintConfig": {
    "extends": ["plugin:prettier/recommended"],
    "rules": {
      "semi": ["error", "never"]
    }
  },
  "eslintIgnore": ["/build/", "/config/", "/dist/", "/test/unit/coverage/"],
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.4"
  },
  "devDependencies": {
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^1.0.0-rc.9",
    "jest": "23.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.13.5",
    "react-native-navigation": "^1.1.471",
    "react-redux": "^5.0.7",
    "react-test-renderer": "16.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
